<div>
  <div class="row">
    <div class="col-sm-6 order-1 order-sm-1">
      <h2 class="d-inline-block">Team {{ Team1.TeamName.String }}</h2> {{> 'partial/winBadge' result=1 }}
    </div>
    <div class="col-sm-6 order-2 order-sm-1 mt-sm-0 mt-4">
      <h2 class="d-inline-block">Team {{ Team2.TeamName.String }}</h2> {{> 'partial/winBadge' result=2 }}
    </div>
    <div class="col-sm-6 order-1 order-sm-1">
